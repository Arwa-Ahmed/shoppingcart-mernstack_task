{
  "name": "shopping-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www ",
    "build": "cd client && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently -n './bin/www , client' \"nodemon ./bin/www\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "engines": {
    "node": "10.14"
  },
  "keywords": [
    "heroku"
  ],
  "dependencies": {
    "bcrypt": "^5.0.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.18",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4"
  }
}
